{
    "version": 2,
    "routes": [
      {
        "src": "/api/v1/auth(.*)",
        "dest": "routes/User.js"
      },
      {
        "src": "/api/v1/profile(.*)",
        "dest": "routes/Profile.js"
      },
      {
        "src": "/api/v1/payment(.*)",
        "dest": "routes/Payment.js"
      },
      {
        "src": "/api/v1/course(.*)",
        "dest": "routes/Course.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  